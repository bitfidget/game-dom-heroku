<h1>Dom Cage Fight</h1>

<p>Drag this to your bookmarks bar. Click it on ANY site when you think things are looking angry!</p>
<p><a href="javascript:var%20i,s,ss=['//cdnjs.cloudflare.com/ajax/libs/jquery/2.1.1/jquery.min.js','http://dom1nation.herokuapp.com/assets/game-dom.js'];for(i=0;i!=ss.length;i++){s=document.createElement('script');s.src=ss[i];document.body.appendChild(s);}void(0);">&lt;dom1nation&gt;</a></p>

<table>
  <thead>
    <tr>
      <th>Score</th>
      <th>Winning Element</th>
      <th>Losing Element</th>
      <th>Content</th>
      <th>favicon</th>
      <th>URL</th>
      <th>Page</th>
    </tr>
  </thead>

  <tbody>
    <% @battles.each do |battle| %>
      <tr>
        <th><%= battle.score %></th>
        <th><%= battle.winner.element %></th>
        <th>
          <% battle.winner.conquests.each do |conquest| %>
            <%= conquest.element %>
          <% end %>
        </th>
        <th><%= battle.winner.content %></th>
        <th><%= battle.winner.page.favicon %></th>
        <th><%= battle.winner.page.url %></th>
        <th><%= battle.winner.page.title %></th>
      </tr>
    <% end %>
  </tbody>

</table>